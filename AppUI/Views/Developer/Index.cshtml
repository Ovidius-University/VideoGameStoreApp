@model IEnumerable<AppUI.Models.ViewModels.CardDeveloperVm>
<section class="row">
    @if (Model.Count() > 0)
    {
        @foreach (var item in Model)
        {
            <article class="me-2 col-lg-2">
                @await Html.PartialAsync("Developer/_Card", item)
            </article>
        }
    }
    else
    {
        <article class="alert alert-warning" role="alert">There is no registered developer!</article>
    }
</section>